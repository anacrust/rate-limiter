{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ActiveProcessorsOptions": {
    "ActiveProcessorNames": [ "LastCallTimeSpan" ]
  },
  "LastCallTimeSpanOptions": {
    "MinRequestTimespanInMilliseconds": 500000
  }
}